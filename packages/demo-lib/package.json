{
  "name": "@jakeklassen/demo-lib",
  "version": "0.3.2",
  "description": "Demo lib",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jakeklassen/demo-lib.git"
  },
  "scripts": {
    "postdocs": "cp jekyll.config.yml docs/config.yml && touch docs/.nojekyll",
    "lint": "eslint src --ext .js,.ts --config .eslintrc.cjs",
    "prebuild": "npm run clean",
    "build": "npm run lint && tsup src --format cjs,esm --dts-resolve",
    "build:watch": "nodemon --exec 'npm run build || exit 1'",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "test": "vitest",
    "pretest:coverage": "npm run clean",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf dist"
  },
  "author": "Jake Klassen <jklassendev@gmail.com>",
  "license": "MIT",
  "keywords": [
    "demo",
    "lib",
    "release",
    "typescript"
  ],
  "devDependencies": {
    "@antfu/ni": "0.18.8",
    "@types/benchmark": "^2.1.2",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.13",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "benchmark": "^2.1.4",
    "bumpp": "8.2.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.0",
    "typescript": "^4.9.4",
    "vite": "4.0.0",
    "vitest": "^0.25.7"
  },
  "dependencies": {
    "tsup": "^6.5.0",
    "type-fest": "3.3.0"
  }
}
