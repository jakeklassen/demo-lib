{
  "name": "@jakeklassen/demo-lib",
  "version": "0.2.1",
  "description": "Demo lib",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jakeklassen/demo-lib.git"
  },
  "scripts": {
    "postdocs": "cp jekyll.config.yml docs/config.yml && touch docs/.nojekyll",
    "lint": "eslint src --ext .js,.ts --config .eslintrc.cjs",
    "prebuild": "npm run clean",
    "build": "npm run lint && tsup src --format cjs,esm --dts-resolve",
    "build:watch": "nodemon --exec 'npm run build || exit 1'",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "test": "vitest",
    "pretest:coverage": "npm run clean",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf dist"
  },
  "author": "Jake Klassen <jklassendev@gmail.com>",
  "license": "MIT",
  "keywords": [
    "demo",
    "lib",
    "release",
    "typescript"
  ],
  "devDependencies": {
    "@antfu/ni": "0.18.0",
    "@types/benchmark": "^2.1.2",
    "@types/eslint": "^8.4.6",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "benchmark": "^2.1.4",
    "bumpp": "8.2.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.3.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.0",
    "typescript": "^4.8.2",
    "vite": "3.0.9",
    "vitest": "^0.22.1"
  },
  "dependencies": {
    "tsup": "^6.2.3",
    "type-fest": "2.19.0"
  }
}
